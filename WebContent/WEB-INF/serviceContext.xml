<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
	
	<bean id="articleCommentService" class="com.createarttechnology.service.impl.ArticleCommentServiceImpl"
		p:articleCommentDao-ref="articleCommentDao" />
		
	<bean id="articleContentService" class="com.createarttechnology.service.impl.ArticleContentServiceImpl"
		p:articleContentDao-ref="articleContentDao" />
		
	<bean id="articleThumbService" class="com.createarttechnology.service.impl.ArticleThumbServiceImpl"
		p:articleThumbDao-ref="articleThumbDao" />
		
	<bean id="userAccountService" class="com.createarttechnology.service.impl.UserAccountServiceImpl"
		p:userAccountDao-ref="userAccountDao" />
		
	<bean id="userInfoService" class="com.createarttechnology.service.impl.UserInfoServiceImpl"
		p:userInfoDao-ref="userInfoDao" />
	
</beans>